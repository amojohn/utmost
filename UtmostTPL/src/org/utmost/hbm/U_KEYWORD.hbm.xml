<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="U_KEYWORD" table="U_KEYWORD">
		<id name="uuid" column="UUID" length="36" type="java.lang.String">
			<generator class="uuid" />
		</id>
		<property name="keyword" type="java.lang.String">
			<column length="1024" name="KEYWORD">
			</column>
		</property>
		<property name="description" type="java.lang.String">
			<column length="2048" name="DESCRIPTION">
			</column>
		</property>
	</class>
</hibernate-mapping>
