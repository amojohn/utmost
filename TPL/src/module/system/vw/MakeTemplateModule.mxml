	<mx:Array id="ds_city">
		<mx:Object label="" value="" />
		<mx:Object label="beijin" value="beijin" />
		<mx:Object label="shanghai" value="shanghai" />
		<mx:Object label="changchun" value="changchun" />
		<mx:Object label="dalian" value="dalian" />
